

public class Profile {
	protected Integer id;
	protected String name;
	
	public Profile(){
		clean();
	}
	
	protected void clean(){
		id=0;
		name = "";
	}
	
	public Integer getId() {
		return id;
	}
	
	public void setId(Integer id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}
